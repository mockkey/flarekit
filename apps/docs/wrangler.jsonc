{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "flarekit-docs",
  "compatibility_date": "2025-04-01",
  "main": "./worker.ts",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "assets": {
    "directory": "./build/client"
  },
  "limits": {
    "cpu_ms": 30000
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}
